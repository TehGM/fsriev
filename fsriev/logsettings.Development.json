{
  "Logging": {
    "MinimumLevel": {
      "Default": "Debug"
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "File",
              "Args": {
                "path": "%PROGRAMDATA%/TehGM/fsriev/logs/log-dev-.log",
                "rollingInterval": "Day",
                "retainedFileCountLimit": 30,
                "fileSizeLimitBytes": 2097152,
                "rollOnFileSizeLimit": true,
                "buffered": false
              }
            }
          ]
        }
      }
    ]
  }
}
